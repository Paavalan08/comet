{
  "name": "comet",
  "productName": "Comet",
  "private": true,
  "version": "2.2.2",
  "description": "A simple media converter",
  "keywords": [
    "comet",
    "video",
    "audio",
    "image",
    "media",
    "converter",
    "ffmpeg"
  ],
  "author": {
    "name": "Jerome Thayananthajothy",
    "email": "tjthavarshan@gmail.com"
  },
  "homepage": "https://comet.thavarshan.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Thavarshan/comet.git"
  },
  "bugs": {
    "url": "https://github.com/Thavarshan/comet/issues"
  },
  "license": "MIT",
  "config": {
    "forge": "./forge.config.ts"
  },
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "npm run start",
    "package": "electron-forge package",
    "make": "electron-forge make --verbose",
    "publish": "electron-forge publish",
    "test": "jest",
    "lint": "eslint '**/*.{js,ts,vue}' --config eslint.config.js --ignore-pattern .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write '**/*.{js,ts,vue}'"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "filesize": "^10.1.6",
    "fluent-ffmpeg": "^2.1.3",
    "jimp": "^1.6.0",
    "lucide-vue-next": "^0.453.0",
    "mobx": "^6.13.5",
    "namor": "^3.0.1",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.1",
    "radix-vue": "^1.9.7",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "unzipper": "^0.12.3",
    "uuid": "^10.0.0",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-appx": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron-forge/plugin-vite": "^7.5.0",
    "@electron-forge/publisher-github": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "@jest/globals": "^29.7.0",
    "@reforged/maker-appimage": "^4.0.2",
    "@rushstack/eslint-patch": "^1.10.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@types/ffmpeg": "^1.0.7",
    "@types/ffprobe-static": "^2.0.3",
    "@types/fluent-ffmpeg": "^2.1.26",
    "@types/jest": "^29.5.13",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "autoprefixer": "^10.4.20",
    "electron": "33.0.1",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.29.1",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "vite": "^5.4.9",
    "vue-eslint-parser": "^9.4.3"
  },
  "engines": {
    "node": ">= 20.10.0"
  },
  "build": {
    "asarUnpack": [
      "node_modules/ffmpeg-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffmpeg-static/index.js",
      "node_modules/ffmpeg-static/package.json",
      "node_modules/ffprobe-static/bin/${os}/${arch}/ffprobe",
      "node_modules/ffprobe-static/index.js",
      "node_modules/ffprobe-static/package.json"
    ]
  }
}
